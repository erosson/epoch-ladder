{
  "name": "elm-application-template",
  "description": "`git clone` this repository for a new Elm Browser.application, including session, page, and route boilerplate",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/erosson/elm-application-template",
  "private": true,
  "scripts": {
    "start": "elm-app start",
    "build": "elm-app build",
    "lint": "elm-format --validate ./src",
    "test": "elm-app test",
    "start:no-debug": "bash -c \"ELM_DEBUGGER=false elm-app start\"",
    "serve": "serve build",
    "serve:build": "bash -c \"yarn build && yarn serve\""
  },
  "devDependencies": {
    "create-elm-app": "^5.22.0",
    "elm-format": "^0.8.4",
    "serve": "^11.3.2"
  }
}
